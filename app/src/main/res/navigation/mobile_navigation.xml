<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.tonala.mabeth.ui.Principal.PrincipalFragment"
        android:label="Menú Principal"
        tools:layout="@layout/fragment_principal">
        <action
            android:id="@+id/action_trans"
            app:destination="@id/nav_inicio"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            />
    </fragment>


    <fragment
        android:id="@+id/nav_Recursos"
        android:name="com.tonala.mabeth.ui.Recursos.RecursosFragment"
        android:label="Recursos"
        tools:layout="@layout/fragment_recursos" />

    <fragment
        android:id="@+id/nav_Calendar"
        android:name="com.tonala.mabeth.ui.Calendario.CalendarFragment"
        android:label="Calendario"
        tools:layout="@layout/fragment_calendar"
        />

    <fragment
        android:id="@+id/nav_homeAd"
        android:name="com.tonala.mabeth.ui.Principal.PrincipalFragment"
        android:label="Menú Principal"
        tools:layout="@layout/fragment_principal"
        />
    <fragment
        android:id="@+id/nav_inicio"
        android:name="com.tonala.mabeth.ui.Principal.PrincipalAdFragment"
        android:label="Inicio"
        tools:layout="@layout/fragment_principal_ad" />

    <fragment
        android:id="@+id/nav_userAd"
        android:name="com.tonala.mabeth.ui.Admin.UsersFragment"
        android:label="Usuarios"
        tools:layout="@layout/fragment_users"/>

    <fragment
        android:id="@+id/nav_adminAd"
        android:name="com.tonala.mabeth.ui.Admin.AdminFragment"
        android:label="Administración"
        tools:layout="@layout/fragment_admin"/>

    <fragment
        android:id="@+id/nav_Gestor"
        android:name="com.tonala.mabeth.ui.Gestor.GestorFragment"
        android:label="Gestor de actividades"
        tools:layout="@layout/fragment_gestor"/>
    <fragment
        android:id="@+id/nav_notas"
        android:name="com.tonala.mabeth.ui.Notas.NotasFragment"
        android:label="Notas"
        tools:layout="@layout/fragment_notas"/>
    <fragment
        android:id="@+id/nav_foros"
        android:name="com.tonala.mabeth.ui.Foros.ForoFragment"
        android:label="Foros"
        tools:layout="@layout/fragment_foro"/>

    <fragment
        android:id="@+id/nav_gps"
        android:name="com.tonala.mabeth.ui.Maps.MapsFragment"
        android:label="Ruta"
        tools:layout="@layout/fragment_maps" />

</navigation>